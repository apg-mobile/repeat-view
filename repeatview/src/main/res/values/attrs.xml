<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="RepeatView">
        <attr name="src" format="reference" />
        <attr name="scaleType" format="enum">
            <enum name="expand" value="0"/>
            <enum name="scaleDown" value="1"/>
        </attr>
        <attr name="scaleDirection" format="enum">
            <enum name="height" value="0"/>
            <enum name="width" value="1"/>
            <enum name="both" value="2"/>
        </attr>
    </declare-styleable>

</resources>